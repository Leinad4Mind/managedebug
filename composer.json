{
	"name": "david63/managedebug",
	"type": "phpbb-extension",
	"description": "This extension will allow the management of the debug options.",
	"version": "3.3.1-RC1",
	"time": "2021-09-25",
		"keywords": [
		"phpbb",
		"extension",
		"debug"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[d63] Manage debug options",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "manage_debug_version_file.json",
			"ssl": false
    	}
	}
}
